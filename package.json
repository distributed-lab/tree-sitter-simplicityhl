{
  "name": "tree-sitter-simplicityhl",
  "version": "0.1.2",
  "description": "Tree-sitter parse for Rust-like high-level language that compiles down to Simplicity bytecode.",
  "repository": "https://github.com/distributed-lab/tree-sitter-simplicityhl.git",
  "license": "MIT",
  "author": {
    "name": "Kyrylo Baibula",
    "email": "kyrylo.baybula@distributed-lab.com"
  },
  "contributors": [
    {
      "name": "Volodymyr Herashchenko",
      "email": "cholkegn@gmail.com"
    }
  ],
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "simplicityhl"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
